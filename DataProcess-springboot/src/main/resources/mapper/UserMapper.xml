<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.DataProcess.springboot.mapper.UserMapper">
<select id="selectUserByuserNameAndpassword" resultType="com.DataProcess.springboot.entity.User">
    SELECT * FROM sys_user WHERE username = #{username} AND password = #{password}
</select>
    <select id="selectUserByuserName" resultType="com.DataProcess.springboot.entity.User">
        SELECT * FROM sys_user WHERE username = #{username}
    </select>
</mapper>
